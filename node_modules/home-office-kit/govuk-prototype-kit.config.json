{
  "meta": {
    "description": "Plugin for the GOV.UK prototype kit to provide Home Office styles",
    "urls": {
      "documentation": "https://github.com/UKHomeOffice/home-office-kit#readme",
      "releaseNotes": "https://github.com/UKHomeOffice/home-office-kit/releases/tag/v{{version}}",
      "versionHistory": "https://github.com/UKHomeOffice/home-office-kit/releases"
    }
  },
  "assets": [
    "/assets"
  ],
  "nunjucksPaths":[
    "/layouts",
    "/macros"
  ],
  "nunjucksFilters": [
    "/filters/all.js",
    "/routes/all.js"
  ],
  "nunjucksMacros": [
    {
      "importFrom": "home-office-kit-legacy-accordion.njk",
      "macroName": "homeOfficeKitLegacyAccordion"
    },
    {
      "importFrom": "home-office-kit-pagination.njk",
      "macroName": "homeOfficeKitPagination"
    }
  ],
  "sass": [
    "/sass/all.scss"
  ],
  "templates": [
    {
      "name": "Blank Home Office page",
      "path": "/templates/home-office-blank.html",
      "type": "nunjucks"
    },
    {
      "name": "Accordion (legacy)",
      "path": "/templates/legacy-accordion.html",
      "type": "nunjucks"
    },
    {
      "name": "Add another",
      "path": "/templates/add-another.html",
      "type": "nunjucks"
    },
    {
      "name": "Pagination",
      "path": "/templates/pagination.html",
      "type": "nunjucks"
    },
    {
      "name": "Question page",
      "path": "/templates/question-page-blank.html",
      "type": "nunjucks"
    },
    {
      "name": "Show password (GOV.UK Frontend v5.3.0 or later)",
      "path": "/templates/show-password-v5-3.html",
      "type": "nunjucks"
    },
    {
      "name": "Show password (GOV.UK Frontend v5.2.0 or earlier)",
      "path": "/templates/show-password.html",
      "type": "nunjucks"
    }
  ],
  "importNunjucksMacrosInto": [
    "/layouts/home-office-kit-layout.html"
  ]
}
